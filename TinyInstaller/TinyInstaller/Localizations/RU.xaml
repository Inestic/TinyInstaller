                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Hyperlink.GitHub.Inestic">https://github.com/Inestic/TinyInstaller</system:String>
    <system:String x:Key="Localization.StartupCondition.ConfigFileExist.Name">Отсутствует файл конфигурации</system:String>
